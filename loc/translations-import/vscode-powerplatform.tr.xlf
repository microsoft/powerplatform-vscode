  <file original="./client/lib/PanelComponents" source-language="en" datatype="plaintext" target-language="tr">
    <body>
      <trans-unit id="pacCLI.AuthProfileTreeItem.toolTipParts.profileKind">
        <source xml:lang="en">Profile Kind: {0}</source>
        <note>The {0} represents the profile type (Admin vs Dataverse)</note>
        <target state="translated">Profil Türü: {0}</target>
      </trans-unit>
      <trans-unit id="pacCLI.AuthProfileTreeItem.toolTipParts.profileName">
        <source xml:lang="en">Name: {0}</source>
        <note>The {0} represents the optional name the user provided for the profile)</note>
        <target state="translated">Ad: {0}</target>
      </trans-unit>
      <trans-unit id="pacCLI.AuthProfileTreeItem.toolTipParts.resource">
        <source xml:lang="en">Resource: {0}</source>
        <note>The {0} represents profile's resource/environment URL</note>
        <target state="translated">Kaynak: {0}</target>
      </trans-unit>
      <trans-unit id="pacCLI.AuthProfileTreeItem.toolTipParts.user">
        <source xml:lang="en">User: {0}</source>
        <note>The {0} represents auth profile's user name (email address))</note>
        <target state="translated">Kullanıcı: {0}</target>
      </trans-unit>
      <trans-unit id="pacCLI.EnvOrSolutionTreeItem.toolTip">
        <source xml:lang="en">Display Name: {0}
Unique Name: {1}
Version: {2}</source>
        <note>This is a multi-line tooltip
The {0} represents Solution's Friendly / Display name
The {1} represents Solution's unique name
The {2} represents Solution's Version number</note>
        <target state="translated">Görünen Ad: {0}
Benzersiz Ad: {1}
Sürüm: {2}</target>
      </trans-unit>
    </body>
  </file>
