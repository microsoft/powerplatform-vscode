  <file original="./client/lib/PanelComponents" source-language="en" datatype="plaintext" target-language="cs">
    <body>
      <trans-unit id="pacCLI.AuthProfileTreeItem.toolTipParts.profileKind">
        <source xml:lang="en">Profile Kind: {0}</source>
        <note>The {0} represents the profile type (Admin vs Dataverse)</note>
        <target state="translated">Druh profilu: {0}</target>
      </trans-unit>
      <trans-unit id="pacCLI.AuthProfileTreeItem.toolTipParts.profileName">
        <source xml:lang="en">Name: {0}</source>
        <note>The {0} represents the optional name the user provided for the profile)</note>
        <target state="translated">Jméno: {0}</target>
      </trans-unit>
      <trans-unit id="pacCLI.AuthProfileTreeItem.toolTipParts.resource">
        <source xml:lang="en">Resource: {0}</source>
        <note>The {0} represents profile's resource/environment URL</note>
        <target state="translated">Prostředek: {0}</target>
      </trans-unit>
      <trans-unit id="pacCLI.AuthProfileTreeItem.toolTipParts.user">
